<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ibreed_project.dao.IJoinDAO">
	<resultMap id="postResult" type="com.ibreed_project.model.JoinVO">
		<result property="postNo" column="postNo" />
		<result property="postCtg" column="postCtg" />
		<result property="postTitle" column="postTitle" />
		<result property="postWriter" column="postWriter" />
		<result property="postDate" column="postDate" jdbcType="TIMESTAMP" />
		<result property="postContent" column="postContent" />
	</resultMap>

	<insert id="joinCommit" parameterType="com.ibreed_project.model.JoinVO">
		INSERT INTO board (postNo, postCtg, postTitle, postWriter, postDate, postContent)
		VALUES (#{postNo}, #{postCtg}, #{postTitle}, #{postWriter}, #{postDate}, #{postContent})
	</insert>


</mapper>
