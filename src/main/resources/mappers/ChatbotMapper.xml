<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ibreed_project.dao.IChatbotDAO">


    <select id="getUserData" resultType="com.ibreed_project.model.ChatbotVO">
        SELECT user_birth, user_nickname, user_kids_select, user_fetus_old, user_kids_old FROM user WHERE user_id = #{user_id}
    </select>

</mapper>

